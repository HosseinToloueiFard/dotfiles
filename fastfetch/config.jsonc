{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "modules": [
    "title",
    "separator",
    "break",
    {
        "type": "custom",
        "format": "\u001b[90m┌──────────────────────Hardware──────────────────────┐"
    },
    {
        "type": "host",
        "key": "🖥 ",
        "keyColor": "yellow"
    },
    {
        "type": "cpu",
        "key": "│ ├󰻠 ",
        "keyColor": "yellow"
    },
    {
        "type": "gpu",
        "key": "│ ├󰍹 ",
        "keyColor": "yellow"
    },
    "break",
    {
        "type": "memory",
        "key": "│ ├󰍛 ",
        "keyColor": "yellow"
    },
    {
        "type": "disk",
        "key": "│ ├🖴 ",
        "keyColor": "yellow"
    },
    {
        "type": "custom",
        "format": "\u001b[90m└────────────────────────────────────────────────────┘"
    },
    "break",
    {
        "type": "custom",
        "format": "\u001b[90m┌──────────────────────Software──────────────────────┐"
    },
    {
        "type": "os",
        "key": " ",
        "keyColor": "green",
    },
    {
        "type": "kernel",
        "key": "│ ├ ",
        "keyColor": "green",
    },
    {
        "type": "packages",
        "key": "│ ├ ",
        "keyColor": "green"
    },
    "break",
    {
        "type": "wm",
        "key": "│ ├ ",
        "keyColor": "green"
    },
    {
        "type": "display",
        "key": "│ ├󰹘 ",
        "keyColor": "green"
    },
    {
        "type": "shell",
        "key": "│ ├ ",
        "keyColor": "green"
    },
    {
        "type": "custom",
        "format": "\u001b[90m└────────────────────────────────────────────────────┘"
    },
    "break",
    {
        "type": "custom",
        "format": "\u001b[90m┌────────────────────System Info────────────────────┐"
    },
    {
        "type": "command",
        "key": "  OS Age ",
        "keyColor": "magenta",
        "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
    },
    {
        "type": "uptime",
        "key": "  Uptime ",
        "keyColor": "magenta"
    },
    {
        "type": "custom",
        "format": "\u001b[90m└────────────────────────────────────────────────────┘"
    },
    "break",
    "colors"
  ]
}
